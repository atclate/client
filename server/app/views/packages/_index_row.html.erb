<tr class="<%= cycle('odd', 'even') -%>">
  <td nowrap="nowrap" width="5%">
    <%= link_to 'View', {:action=>"show", :id => index_row.id}  %> 
  </td>
  <td><%= h(index_row.name) %></td>
  <td><%= h(index_row.version) %></td>
  <td><%= h(index_row.package_version) %></td>
  <td> <%= link_to h(index_row.filename), {:action=>"download", :filename => index_row.filename}  %></td>
  <td><%= h(index_row.maintainer) %></td>
  <td><%= h(index_row.os) %></td>
  <td><%= h(index_row.arch) %></td>
  <td><%= link_to "#{h(index_row.client_packages.size)} client(s)",{:action=>"show", :id => index_row.id} %></td>
</tr>
